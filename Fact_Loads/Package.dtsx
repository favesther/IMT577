      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{8AE4C0A9-DE7D-4438-8264-A078AAE862D7}"
          SQLTask:SqlStatementSource="IF EXISTS (SELECT TABLE_NAME FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_SCHEMA = 'dbo' AND TABLE_NAME = 'factProductSalesTarget')&#xA;BEGIN&#xA;&#x9;-- ====================================&#xA;&#x9;-- Load factProductSalesTarget table&#xA;&#x9;-- ====================================&#xA;&#xA;&#x9;INSERT INTO dbo.factProductSalesTarget&#xA;&#x9;(&#xA;    dimProductKey&#xA;    dimTargetDateKey&#xA;    ProductTargetSalesAmount&#xA;&#x9;)&#xA;&#x9;SELECT p.dimProductKey&#xA;    , d.DimDateID as dimTargetDateKey&#xA;    ,CAST(pt.SalesQuantityTarget AS int)/365.0 as ProductTargetSalesAmount&#xA;    FROM dbo.StageProductTarget pt, dbo.dimProduct p, dbo.DimDate d&#xA;    WHERE pt.Year = d.CalendarYear&#xA;END&#xA;GO " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
