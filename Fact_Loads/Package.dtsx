        <SQLTask:SqlTaskData
          SQLTask:Connection="{8AE4C0A9-DE7D-4438-8264-A078AAE862D7}"
          SQLTask:SqlStatementSource="IF EXISTS (SELECT TABLE_NAME FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_SCHEMA = 'dbo' AND TABLE_NAME = 'factSRCSalesTarget')&#xA;BEGIN&#xA;&#x9;DELETE FROM dbo.factSRCSalesTarget;&#xA;END&#xA;GO&#xA;&#xA;&#xA;&#xA;IF NOT EXISTS (SELECT TABLE_NAME FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_SCHEMA = 'dbo' AND TABLE_NAME = 'factSRCSalesTarget')&#xA;BEGIN&#xA;&#x9;CREATE TABLE dbo.factSRCSalesTarget&#xA;&#x9;(&#xA;&#x9;factSalesTarget INT IDENTITY(1,1) CONSTRAINT PK_factSRCSalesTarget PRIMARY KEY CLUSTERED NOT NULL, -- SurrogateKey&#xA;    dimStoreKey INT NOT NULL CONSTRAINT FK_factSRCSalesTarget_dimStore FOREIGN KEY REFERENCES dbo.dimStore (dimStoreKey),&#x9;&#xA;    dimResellerKey INT NOT NULL CONSTRAINT FK_factSRCSalesTarget_dimReseller FOREIGN KEY REFERENCES dbo.dimReseller (dimResellerKey),&#xA;&#x9;dimChannelID INT NOT NULL CONSTRAINT FK_factSRCSalesTarget_dimChannel FOREIGN KEY REFERENCES dbo.dimChannel (dimChannelID),&#xA;&#x9;dimTargetDateKey INT NOT NULL CONSTRAINT FK_factSRCSalesTarget_DimDate FOREIGN KEY REFERENCES dbo.DimDate (DimDateID),&#xA;&#x9;dimTargetSalesAmount numeric(16,6) NOT NULL&#xA;&#x9;);&#xA;END&#xA;GO&#xA;&#xA;IF EXISTS (SELECT TABLE_NAME FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_SCHEMA = 'dbo' AND TABLE_NAME = 'factSRCSalesTarget')&#xA;BEGIN&#xA;&#x9;-- ====================================&#xA;&#x9;-- Load factSRCSalesTarget table&#xA;&#x9;-- ====================================&#xA;&#xA;&#x9;INSERT INTO dbo.factSRCSalesTarget&#xA;&#x9;(&#xA;    dimStoreKey&#xA;    ,dimResellerKey&#xA;    ,dimChannelID&#xA;    ,dimTargetDateKey&#xA;    ,dimTargetSalesAmount&#xA;&#x9;)&#xA;&#x9;SELECT &#xA;&#x9;dimStore.dimStoreKey&#xA;&#x9;,dimReseller.dimResellerKey&#xA;&#x9;,dimChannel.dimChannelID&#xA;&#x9;,DimDate.DimDateID AS dimTargetDateKey&#xA;    ,CAST(StageTargetCRS.TargetSalesAmount AS int)/365.0 AS dimTargetSalesAmount&#xA;&#xA;&#x9;FROM dbo.StageTargetCRS StageTargetCRS, &#xA;    dbo.dimChannel dimChannel, &#xA;    dbo.dimReseller dimReseller,&#xA;    dbo.DimDate DimDate,&#xA;    dbo.dimStore dimStore&#xA;    -- (SELECT concat(&quot;Store Number &quot;,dimStoreNumber) AS StoreName, dimStoreKey FROM dbo.dimStore) AS store&#xA;    WHERE StageTargetCRS.ChannelName = dimChannel.dimChannelName &#xA;    AND StageTargetCRS.TargetName = dimReseller.dimResellerName &#xA;    AND StageTargetCRS.TargetName = &quot;Store Number &quot; + dimStore.dimStoreNumber&#xA;    AND StageTargetCRS.Year = DimDate.CalendarYear;&#xA;    &#xA;END&#xA;GO &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
