      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{8AE4C0A9-DE7D-4438-8264-A078AAE862D7}"
          SQLTask:SqlStatementSource="IF EXISTS (SELECT TABLE_NAME FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_SCHEMA = 'dbo' AND TABLE_NAME = 'factSRCSalesTarget')&#xA;BEGIN&#xA;&#x9;-- ====================================&#xA;&#x9;-- Load factSRCSalesTarget table&#xA;&#x9;-- ====================================&#xA;&#xA;&#x9;INSERT INTO dbo.factSRCSalesTarget&#xA;&#x9;(&#xA;    dimStoreKey&#xA;    ,dimResellerKey&#xA;    ,dimChannelID&#xA;    ,dimTargetDateKey&#xA;    ,TargetSalesAmount&#xA;&#x9;)&#xA;&#x9;SELECT &#xA;&#x9;dimStore.dimStoreKey&#xA;&#x9;,dimReseller.dimResellerKey&#xA;&#x9;,dimChannel.dimChannelID&#xA;&#x9;,DimDate.DimDateID AS dimTargetDateKey&#xA;    ,CAST(StageTargetCRS.TargetSalesAmount/365.0 AS numeric(16,6)) AS TargetSalesAmount&#xA;&#xA;&#x9;FROM dbo.StageTargetCRS As StageTargetCRS&#xA;    INNER JOIN dbo.dimChannel as dimChannel&#xA;    ON StageTargetCRS.ChannelName = dimChannel.dimChannelName &#xA;    INNER JOIN dbo.dimReseller as dimReseller&#xA;    StageTargetCRS.TargetName = dimReseller.dimResellerName&#xA;    INNER JOIN dbo.DimDate as DimDate&#xA;    on StageTargetCRS.Year = DimDate.CalendarYear&#xA;    INNER JOIN dbo.dimStore as dimStore &#xA;    on StageTargetCRS.TargetName = dimStore.dimStoreName;&#xA;    &#xA;END&#xA;GO " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
